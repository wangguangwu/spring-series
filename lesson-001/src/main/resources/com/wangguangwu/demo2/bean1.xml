<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"

       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <!--通过 id 定义 bean 名称-->
    <bean id="user1" class="com.wangguangwu.demo2.UserModel"/>

    <!--通过 name 定义 bean 名称-->
    <bean name="user2" class="com.wangguangwu.demo2.UserModel"/>

    <!--id 为名称，name 为别名-->
    <bean id="user3" name="user3_1" class="com.wangguangwu.demo2.UserModel"/>

    <!--id 为名称，name 为别名-->
    <bean id="user4" name="user4_1,user4_2;user4_3 user4_4" class="com.wangguangwu.demo2.UserModel"/>

    <!--bean 名称：user5，别名：...-->
    <bean name="user5,user5_1,user5_2;user5_3 use5_4" class="com.wangguangwu.demo2.UserModel"/>

</beans>